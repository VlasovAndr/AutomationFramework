{
  "framework": {
    "Enviroment": "qa", // [qa/qa02] Choose enviroment for testrun
    "DownloadedLocation": "", // Default is blank & consider under Download Folder. User can provide there location to download data.
    "StepScreenShot": "off", // [on/off] Default is off. To get screen shot for each step mention ON otherwise screen shot of failure will be added in extent report.
    "ExtentReportPortalUrl": "", // Default as mentioned. Recommended to have seperate report portal setup for each team.
    "ExtentReportToPortal": "off", // [on/off] Default is off. Report push to portal if mentioned ON.
    "LogLevel": "INFORMATION" // [ALL/DEBUG/INFORMATION/WARN/ERROR/FATAL] Default:"DEBUG. ALL < DEBUG < INFORMATION < WARN < ERROR < FATAL.
  },

  "driver": {
    "BrowserName": "chrome", // [chrome/firefox] Default BrowserName: Chrome 
    "BrowserType": "local", // [local/remote] BrowserType local
    "GridHubUrl": "http://selenoid:4444/wd/hub", // REMOTE - [http://selenoid:4444/wd/hub / LOCAL - http://localhost:4444/wd/hub]
    "WaitSeconds": 10,
    "Headless": false
  },

  "enviroment": {
    "qa": {
